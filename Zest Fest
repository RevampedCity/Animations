local XanaxUILib = loadstring(game:HttpGet("https://pastebin.com/raw/XZz3Ytbu"))()
local Ui = XanaxUILib:CreateWindow("Revamped.City | Zest Fest")
local R6Tab, R15Tab, R6TrollTab, AnimationsTab = Ui:CreateSection("R6 Sexual"), Ui:CreateSection("R15 Sexual"), Ui:CreateSection("R6 Troll"), Ui:CreateSection("Animations")
local r6Scripts = {["Fuck Someone"]="https://pastebin.com/raw/aPSHMV6K",["Get Fucked"]="https://pastebin.com/raw/zHbw7ND1",["Get Head"]="https://pastebin.com/raw/FPu4e2Qh",["Stroke Dick"]="https://pastefy.app/wa3v2Vgm/raw",["Hug - H to Hug"]="https://raw.githubusercontent.com/RevampedCity/Animations/refs/heads/main/Hug",["Hand Job"]="https://raw.githubusercontent.com/RevampedCity/Animations/refs/heads/main/Slurp%20Cock"}
local r15Scripts = {["Fuck Someone"]="https://pastebin.com/raw/1ePMTt9n",["Get Fucked"]="https://pastebin.com/raw/7hvcjDnW",["Get Head"]="https://pastebin.com/raw/DyPP2tAF",["Stroke Dick"]="https://pastefy.app/YZoglOyJ/raw"}
local function executeScript(name, type) local s = type == "R6" and r6Scripts[name] or r15Scripts[name] if s then local ok, e = pcall(function() loadstring(game:HttpGet(s))() end) if not ok then warn("Error:" .. e) end end end

R6TrollTab:CreateButton("Flip", function()
    local ca, zeezy, h = game:GetService("ContextActionService"), game:GetService("Players").LocalPlayer, 0.0174533
    local function zeezyFlip(dir) return function(_, inp) if inp == Enum.UserInputState.Begin then zeezy.Character.Humanoid:ChangeState("Jumping") wait() zeezy.Character.Humanoid.Sit = true for i = 1, 360 do delay(i / 720, function() zeezy.Character.HumanoidRootPart.CFrame = zeezy.Character.HumanoidRootPart.CFrame * CFrame.Angles(h * dir, 0, 0) end) end wait(0.55) zeezy.Character.Humanoid.Sit = false end end end
    ca:BindAction("zeezyFrontflip", zeezyFlip(-1), false, Enum.KeyCode.Z)
    ca:BindAction("zeezyBackflip", zeezyFlip(1), false, Enum.KeyCode.X)
    ca:BindAction("zeezyAirjump", function(_, inp) if inp == Enum.UserInputState.Begin then local h = zeezy.Character.Humanoid h:ChangeState("Seated") wait() h:ChangeState("Jumping") end end, false, Enum.KeyCode.C)
end)

R6TrollTab:CreateButton("Spiderman", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/RevampedCity/Animations/refs/heads/main/Spider%20Man"))() end)
R6TrollTab:CreateButton("Snail", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/RevampedCity/Animations/refs/heads/main/Snail"))() end)
for name, url in pairs(r6Scripts) do R6Tab:CreateButton(name, function() executeScript(name, "R6") end) end
for name, url in pairs(r15Scripts) do R15Tab:CreateButton(name, function() executeScript(name, "R15") end) end
AnimationsTab:CreateButton("Ban Hammer", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/ShutUpJamesTheLoser/Banhammer/refs/heads/main/ban", true))() end)
AnimationsTab:CreateButton("Drift Car", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/RevampedCity/Animations/refs/heads/main/FE%20Drift%20Car"))() end)

local deletionEnabled, deletions, mouse = false, {}, game:GetService("Players").LocalPlayer:GetMouse()
mouse.Button1Down:Connect(function() if deletionEnabled and game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.C) and mouse.Target then local t = mouse.Target table.insert(deletions, {ClassName = t.ClassName, Size = t.Size, Position = t.Position, Rotation = t.Rotation, BrickColor = t.BrickColor, Material = t.Material, Anchored = t.Anchored}) t:Destroy() end end)
AnimationsTab:CreateToggle("C Delete", "DeletionToggle", function(e) deletionEnabled = e if not e then for _, o in ipairs(deletions) do local r = Instance.new(o.ClassName) r.Size, r.Position, r.Rotation, r.BrickColor, r.Material, r.Anchored, r.Parent = o.Size, o.Position, o.Rotation, o.BrickColor, o.Material, o.Anchored, workspace end end end)
